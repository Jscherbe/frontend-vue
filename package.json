{
  "name": "@ulu/frontend-vue",
  "version": "0.2.0-beta.7",
  "description": "A modular and tree-shakeable Vue 3 component library for the Ulu frontend",
  "type": "module",
  "files": [
    "dist",
    "lib"
  ],
  "module": "./dist/index.js",
  "sideEffects": [
    "dist/composables/useBreakpointManager.js",
    "dist/composables/useDocumentTitle.js",
    "dist/composables/useWindowResize.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./scss": "./lib/_index.scss",
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jscherbe/frontend-vue.git"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "docs:build": "storybook build -o docs",
    "build": "vite build --config library.vite.config.js",
    "deploy": "npm run build && npm run docs:build",
    "update-contexts": "rm -rf .ctx && mkdir -p .ctx/frontend && cp -R node_modules/@ulu/frontend/lib/scss node_modules/@ulu/frontend/lib/js .ctx/frontend/"
  },
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "components",
    "component-library",
    "design-system",
    "ulu",
    "frontend",
    "scss"
  ],
  "author": "Joseph Scherben <jscherbe@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jscherbe/frontend-vue/issues"
  },
  "homepage": "https://github.com/Jscherbe/frontend-vue#readme",
  "peerDependencies": {
    "@floating-ui/vue": "^1.1.8",
    "@formkit/auto-animate": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@headlessui/vue": "^1.7.23",
    "@portabletext/vue": "^1.0.14",
    "@ulu/frontend": "^0.2.0-beta.11",
    "@ulu/utils": "^0.0.34",
    "@unhead/vue": "^2.0.11",
    "fuse.js": "^6.6.2",
    "gsap": "^3.13.0",
    "lodash-es": "^4.17.21",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue3-dropzone": "^2.2.1"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.1.8",
    "@formkit/auto-animate": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@headlessui/vue": "^1.7.23",
    "@portabletext/vue": "^1.0.14",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-links": "^9.1.1",
    "@storybook/vue3-vite": "^9.1.1",
    "@ulu/frontend": "^0.2.0-beta.11",
    "@ulu/utils": "^0.0.34",
    "@unhead/vue": "^2.0.11",
    "@vitejs/plugin-vue": "^6.0.0",
    "fuse.js": "^6.6.2",
    "glob": "^13.0.0",
    "gsap": "^3.13.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass-embedded": "^1.89.2",
    "storybook": "^9.1.1",
    "storybook-addon-vue-mdx": "^2.0.2",
    "typescript": "^5.3.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.6",
    "vue3-dropzone": "^2.2.1"
  },
  "volta": {
    "node": "20.19.6"
  }
}
